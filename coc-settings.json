{
  "languageserver": {
    "flow": {
      "command": "flow",
      "args": ["lsp"],
      "filetypes": ["javascript", "javascriptreact"],
      "initializationOptions": {},
      "requireRootPattern": true,
      "settings": {},
      "rootPatterns": [".flowconfig"]
    }
  },
  "explorer.icon.enableNerdfont":true,
  "explorer.keyMappings.global":{
    "<cr>":["expandable?", ["expanded?", "collapse", "expand"], "open"]
  },
  "diagnostic-languageserver.filetypes":{"go":"golangci-lint"},
  "diagnostic-languageserver.linters":{
    "golangci-lint": {
      "args": [ "run", "--out-format", "json", "." ]
    }
  },
  "diagnostic-languageserver.formatters":{"goreturns":{"command":"goreturns"}},
  "diagnostic-languageserver.formatFiletypes":{"go":"goreturns"},
  "coc.preferences.formatOnSaveFiletypes":["go", "javascript","javascriptreact","typescript","typescriptreact"],
  "tsserver.formatOnType": true,
  "coc.preferences.formatOnType": true,
  "eslint.autoFixOnSave": true,
  "eslint.filetypes": ["javascript", "javascriptreact", "typescript", "typescriptreact"]
}
